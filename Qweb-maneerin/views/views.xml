<odoo>
    <record id="maneerincode_view_form" model="ir.ui.view">
        <field name="name">maneerincode Form</field>
        <field name="model">maneerincode.code</field>
        <field name="arch" type="xml">
            <form string="maneerincode Form">
                <sheet>
                    <group>
                        <field name="contract_no"/>  <!-- ฟิลด์เลขที่สัญญา -->
                        <field name="contact_date"/>  <!-- ฟิลด์วันที่ติดต่อ -->
                        <field name="seller"/>  <!-- ฟิลด์ผู้ขาย -->
                        <field name="buyer"/>   <!-- ฟิลด์ผู้ซื้อ -->
                        <field name="beneficiary"/>  <!-- ฟิลด์ผู้รับสิทธิ์ -->
                    </group>                
                </sheet>
            </form>
        </field>
    </record>

    <record id="maneerincode_view_tree" model="ir.ui.view">
        <field name="name">maneerincode Tree</field>
        <field name="model">maneerincode.code</field>
        <field name="arch" type="xml">
            <tree string="maneerincode List">
                <field name="contract_no"/>  <!-- ฟิลด์เลขที่สัญญา -->
                <field name="contact_date"/>  <!-- ฟิลด์วันที่ติดต่อ -->
                <field name="seller"/>  <!-- ฟิลด์ผู้ขาย -->
                <field name="buyer"/>   <!-- ฟิลด์ผู้ซื้อ -->
                <field name="beneficiary"/>  <!-- ฟิลด์ผู้รับสิทธิ์ -->
            </tree>
        </field>
    </record>

    <record id="maneerincode_action" model="ir.actions.act_window">
        <field name="name">maneerincode</field>
        <field name="res_model">maneerincode.code</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="maneerincode_menu" name="maneerincode" sequence="1" action="maneerincode_action"/>
</odoo>

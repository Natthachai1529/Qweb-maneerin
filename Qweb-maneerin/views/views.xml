<odoo>
    <record id="view_maneerincode_form" model="ir.ui.view">
        <field name="name">maneerincode.form</field>
        <field name="model">maneerincode.code</field>
        <field name="arch" type="xml">
            <form string="Maneerin Code">
                <sheet>
                    <group>
                        <field name="contract_no"/>
                        <field name="contact_date"/>
                        <field name="seller"/>
                        <field name="buyer_id"/>
                        <field name="beneficiary_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_maneerincode_tree" model="ir.ui.view">
        <field name="name">maneerincode.tree</field>
        <field name="model">maneerincode.code</field>
        <field name="arch" type="xml">
            <tree string="Maneerin Code">
                <field name="contract_no"/>
                <field name="contact_date"/>
                <field name="seller"/>
                <field name="buyer_id"/>
                <field name="beneficiary_id"/>
            </tree>
        </field>
    </record>

    <record id="action_maneerincode" model="ir.actions.act_window">
        <field name="name">Maneerin Code</field>
        <field name="res_model">maneerincode.code</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_maneerincode" name="Maneerin Code" action="action_maneerincode"/>
</odoo>
